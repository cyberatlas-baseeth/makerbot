# StandX Market Maker - Environment Variables
# Copy this file to .env and fill in values

# Wallet private key (hex, without 0x prefix)
PRIVATE_KEY=your_private_key_here

# StandX API endpoints (optional, overrides config.yaml)
STANDX_API_URL=https://api.standx.io
STANDX_WS_URL=wss://ws.standx.io

